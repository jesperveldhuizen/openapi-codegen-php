{
  "name" : "elastic/openapi-codegen",
  "description" : "A base library used to create Elastic PHP clients.",
  "homepage": "https://github.com/elastic/openapi-codegen-php",
  "keywords" : [
    "search",
    "client",
    "elastic",
    "openapi"
  ],
  "license" : "Apache-2.0",
  "type" : "library",
  "authors" : [
    {"name" : "Aur√©lien FOUCRET", "email": "aurelien.foucret@elastic.co"}
  ],
  "require" : {
    "php" : "^7.4|^8.0",
    "guzzlehttp/ringphp" : "^1.0",
    "psr/log" : "^1.0."
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "2.18.*",
    "phpstan/phpstan": "^1.0",
    "symplify/easy-coding-standard": "9.2.*",
    "symplify/symplify-kernel": "9.2.*"
  },
  "autoload" : {
    "psr-4" : {
      "Elastic\\OpenApi\\Codegen\\" : ""
    },
    "exclude-from-classmap" : [
      "/Tests/"
    ]
  },
  "bin": [
      "resources/dev/elastic-openapi-codegen.sh"
  ]
}
